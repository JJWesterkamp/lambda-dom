{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "lib": ["dom", "es5", "es6", "es7"],
        "noImplicitAny": true,
        "sourceMap": true,
        "esModuleInterop": true,
        "strictNullChecks": true,
        "downlevelIteration": true,
        "importHelpers": true
    },
    "include": ["src/**/*.ts"]
}
