  },
  "devDependencies": {
    "@types/webpack-env": "^1.15.3",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^8.0.11",
    "typescript": "^4.0.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.4.0",
